#:kivy 1.0.9

<Button>
    font_size: 30
    color: 0.3, 0.6, 0.7, 1
    size_hint: 0.3, 0.5


<FloatLayout>:
    Button:
        pos_hint: {'x': 0.5, 'top': 1}
        text: 'Hello World'

    Button:
        id: btn
        text: 'Gabriel' if btn.state == 'normal' else 'Kivy'
        pos_hint: {'y': 0.5}

